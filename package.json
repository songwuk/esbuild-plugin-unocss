{
  "name": "tsup-plugin-unocss",
  "type": "module",
  "version": "0.1.8",
  "packageManager": "pnpm@7.1.5",
  "description": "tsup plguin for unocss",
  "keywords": [
    "vue",
    "tsup",
    "unocss"
  ],
  "author": "Song wuk <1017301325@qq.com>",
  "license": "MIT",
  "homepage": "https://github.com/songwuk/tsup-plugin-unocss#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/songwuk/tsup-plugin-unocss.git"
  },
  "bugs": "https://github.com/songwuk/tsup-plugin-unocss/issues",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup --dts",
    "lint": "eslint .",
    "prepublishOnly": "nr build",
    "release": "bumpp --commit --push --tag && npm publish",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.24.2",
    "@antfu/ni": "^0.16.2",
    "@antfu/utils": "^0.5.2",
    "@types/fs-extra": "^9.0.13",
    "@types/node": "^17.0.35",
    "bumpp": "^7.1.1",
    "esbuild": "^0.14.10",
    "eslint": "^8.16.0",
    "pnpm": "^7.1.5",
    "rimraf": "^3.0.2",
    "tsup": "^6.0.1",
    "typescript": "^4.7.2",
    "vite": "^2.9.9",
    "vitest": "^0.12.9"
  },
  "dependencies": {
    "@unocss/core": "^0.34.1",
    "@unocss/preset-uno": "^0.34.1",
    "fs-extra": "^10.0.0"
  }
}
